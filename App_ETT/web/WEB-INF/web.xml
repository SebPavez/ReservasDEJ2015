<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>AgregarCliente</servlet-name>
        <servlet-class>servlets.AgregarCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarReserva</servlet-name>
        <servlet-class>servlets.AgregarReserva</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarCliente</servlet-name>
        <servlet-class>servlets.BuscarCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Modificar</servlet-name>
        <servlet-class>servlets.ModificarCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>buscarEntreFechas</servlet-name>
        <servlet-class>servlets.buscarEntreFechas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AgregarCliente</servlet-name>
        <url-pattern>/AgregarCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarReserva</servlet-name>
        <url-pattern>/AgregarReserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarCliente</servlet-name>
        <url-pattern>/BuscarCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Modificar</servlet-name>
        <url-pattern>/Modificar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>buscarEntreFechas</servlet-name>
        <url-pattern>/buscarEntreFechas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>paginas/agregarReserva.jsp</welcome-file>
    </welcome-file-list>
</web-app>
